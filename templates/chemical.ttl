@prefix bgf: <https://bg-rdf.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

#===============================================================================

bgf:Chemical
    a bgf:PhysicalDomain ;
    bgf:hasConstant [
        bgf:varName "RT" ;
        bgf:hasValue "2578.73058 J/mol"^^cdt:ucum
    ], [
        bgf:varName "F" ;
        rdfs:label "Faraday's constant" ;
        bgf:hasValue "0.96485e5 C/mol"^^cdt:ucum
    ] ;
    bgf:hasFlow [
        bgf:varName "v" ;
        bgf:hasUnits "mol/s"^^cdt:ucumunit
    ] ;
    bgf:hasPotential [
        bgf:varName "u" ;
        bgf:hasUnits "J/mol"^^cdt:ucumunit
    ] ;
    bgf:hasQuantity [
        bgf:varName "q" ;
        rdfs:label "Chemical quantity (mol)" ;
        bgf:hasUnits "mol"^^cdt:ucumunit
    ] .


#===============================================================================

bgf:ChemicalFlow
    a bgf:ElementTemplate ;
    rdfs:subClassOf bgf:FlowSource ;
    bgf:hasDomain bgf:Chemical .

#===============================================================================

bgf:ChemicalReaction
    a bgf:ElementTemplate ;
    rdfs:subClassOf bgf:Dissipator ;
    bgf:hasDomain bgf:Chemical ;
    rdfs:label "Chemical reaction" ;
    bgf:hasPort 'f', 'r' ;
    bgf:hasParameter [
        bgf:varName "kappa" ;
        bgf:hasUnits "mol/s"^^cdt:ucumunit
    ] ;
    # v = kappa*(exp(u_f/RT) - exp(u_r/RT))
    bgf:constitutiveRelation """
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>v</ci>
                <apply>
                    <times/>
                    <ci>kappa</ci>
                    <apply>
                        <minus/>
                        <apply>
                            <exp/>
                            <apply>
                                <divide/>
                                <ci>u_f</ci>
                                <ci>RT</ci>
                            </apply>
                        </apply>
                        <apply>
                            <exp/>
                            <apply>
                                <divide/>
                                <ci>u_r</ci>
                                <ci>RT</ci>
                            </apply>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    """^^bgf:mathml .

#===============================================================================

bgf:ChemicalConcentration
    a bgf:ElementTemplate ;
    rdfs:subClassOf bgf:QuantityStore ;
    bgf:hasDomain bgf:Chemical ;
    rdfs:label "Chemical concentration" ;
    bgf:hasParameter [
        bgf:varName "K" ;
        bgf:hasUnits "mol-1"^^cdt:ucumunit
    ] ;
    # ode(q, t) = v
    # u = RT*ln(K*q);
    bgf:constitutiveRelation """
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q</ci>
                </apply>
                <ci>v</ci>
            </apply>
            <apply>
                <eq/>
                <ci>u</ci>
                <apply>
                    <times/>
                    <ci>RT</ci>
                    <apply>
                        <ln/>
                        <apply>
                            <times/>
                            <ci>K</ci>
                            <ci>q</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    """^^bgf:mathml .

#===============================================================================

bgf:NKETransporter
    a bgf:ElementTemplate ;
    rdfs:subClassOf bgf:Dissipator ;
    bgf:hasDomain bgf:Chemical ;
    bgf:hasPort [
        bgf:portId 'K_f' ;
        bgf:bondCount 2
    ], [
        bgf:portId 'K_r' ;
        bgf:bondCount 2
    ], [
        bgf:portId 'Na_f' ;
        bgf:bondCount 3
    ], [
        bgf:portId 'Na_r' ;
        bgf:bondCount 3
    ] ;
    bgf:hasParameter [
        bgf:varName "k_NKE" ;
        bgf:hasUnits "mol/s"^^cdt:ucumunit
    ] ;
    bgf:hasVariable [
        bgf:varName "u_e";
        bgf:hasUnits "J/C"^^cdt:ucumunit
    ], [
        bgf:varName "q_H2O_in" ;
        bgf:hasUnits "L"^^cdt:ucumunit
    ], [
        bgf:varName "q_H2O_out" ;
        bgf:hasUnits "L"^^cdt:ucumunit
    ], [
        bgf:varName "K_Na" ;
        bgf:hasUnits "L/mol"^^cdt:ucumunit
    ], [
        bgf:varName "q_Na_in" ;
        bgf:hasUnits "mol"^^cdt:ucumunit
    ], [
        bgf:varName "q_Na_out" ;
        bgf:hasUnits "mol"^^cdt:ucumunit
    ], [
        bgf:varName "K_K";
        bgf:hasUnits "L/mol"^^cdt:ucumunit
    ], [
        bgf:varName "q_K_in" ;
        bgf:hasUnits "mol"^^cdt:ucumunit
    ], [
        bgf:varName "q_K_out" ;
        bgf:hasUnits "mol"^^cdt:ucumunit
    ] ;
    bgf:constitutiveRelation """
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>v</ci>
                <apply>
                    <divide/>
                    <apply>
                        <times/>
                        <ci>k_NKE</ci>
                        <cn>3</cn>
                        <apply>
                            <minus/>
                            <apply>
                                <times/>
                                <apply>
                                    <power/>
                                    <apply>
                                        <divide/>
                                        <apply>
                                            <times/>
                                            <ci>K_Na</ci>
                                            <ci>q_Na_in</ci>
                                        </apply>
                                        <ci>q_H2O_in</ci>
                                    </apply>
                                    <cn>3</cn>
                                </apply>
                                <apply>
                                    <power/>
                                    <apply>
                                        <divide/>
                                        <apply>
                                            <times/>
                                            <ci>K_K</ci>
                                            <ci>q_K_out</ci>
                                        </apply>
                                        <ci>q_H2O_out</ci>
                                    </apply>
                                    <cn>2</cn>
                                </apply>
                            </apply>
                            <apply>
                                <times/>
                                <apply>
                                    <power/>
                                    <apply>
                                        <divide/>
                                        <apply>
                                            <times/>
                                            <ci>K_Na</ci>
                                            <ci>q_Na_out</ci>
                                        </apply>
                                        <ci>q_H2O_out</ci>
                                    </apply>
                                    <cn>3</cn>
                                </apply>
                                <apply>
                                    <power/>
                                    <apply>
                                        <divide/>
                                        <apply>
                                            <times/>
                                            <ci>K_K</ci>
                                            <ci>q_K_in</ci>
                                        </apply>
                                        <ci>q_H2O_in</ci>
                                    </apply>
                                    <cn>2</cn>
                                </apply>
                                <apply>
                                    <exp/>
                                    <apply>
                                        <divide/>
                                        <apply>
                                            <times/>
                                            <cn>2</cn>
                                            <ci>F</ci>
                                            <ci>u_e</ci>
                                        </apply>
                                        <ci>RT</ci>
                                    </apply>
                                </apply>
                            </apply>
                        </apply>
                    </apply>
                    <apply>
                        <times/>
                        <apply>
                            <plus/>
                            <cn>1</cn>
                            <apply>
                                <power/>
                                <apply>
                                    <divide/>
                                    <apply>
                                        <times/>
                                        <ci>K_Na</ci>
                                        <ci>q_Na_in</ci>
                                    </apply>
                                    <ci>q_H2O_in</ci>
                                </apply>
                                <cn>3</cn>
                            </apply>
                        </apply>
                        <apply>
                            <plus/>
                            <cn>1</cn>
                            <apply>
                                <power/>
                                <apply>
                                    <divide/>
                                    <apply>
                                        <times/>
                                        <ci>K_Na</ci>
                                        <ci>q_Na_out</ci>
                                    </apply>
                                    <ci>q_H2O_out</ci>
                                </apply>
                                <cn>3</cn>
                            </apply>
                        </apply>
                        <apply>
                            <plus/>
                            <cn>1</cn>
                            <apply>
                                <power/>
                                <apply>
                                    <divide/>
                                    <apply>
                                        <times/>
                                        <ci>K_K</ci>
                                        <ci>q_K_out</ci>
                                    </apply>
                                    <ci>q_H2O_out</ci>
                                </apply>
                                <cn>2</cn>
                            </apply>
                        </apply>
                        <apply>
                            <plus/>
                            <cn>1</cn>
                            <apply>
                                <power/>
                                <apply>
                                    <divide/>
                                    <apply>
                                        <times/>
                                        <ci>K_K</ci>
                                        <ci>q_K_in</ci>
                                    </apply>
                                    <ci>q_H2O_in</ci>
                                </apply>
                                <cn>2</cn>
                            </apply>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    """^^bgf:mathml .

#===============================================================================

bgf:ChemicalStorageNode
    a bgf:CompositeElement ;
    rdfs:label "Chemical storage with Zero Node" ;
    rdfs:subClassOf bgf:ChemicalConcentration .

#===============================================================================

bgf:ChemicalReactionNode
    a bgf:CompositeElement ;
    rdfs:label "Chemical reaction withe One Node" ;
    rdfs:subClassOf bgf:ChemicalReaction .

#===============================================================================

bgf:NKETransporterNode
    a bgf:CompositeElement ;
    rdfs:label "NKE transporter with One Node" ;
    rdfs:subClassOf bgf:NKETransporter .

