@prefix bgf: <https://bg-rdf.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .
@prefix tpl: <https://bg-rdf.org/templates/> .

@prefix : <#> .

#===============================================================================
#
#           C ------> R ------> L
#
# Simulation: 100 seconds with a 1 second step
#
#===============================================================================

:example_A1
    a bgf:BondgraphModel ;
    bgf:usesTemplate tpl:electrical.ttl ;
    bgf:hasBondElement :C, :R, :L .

#===============================================================================

:C
    a bgf:ElectricalCapacitiveNode ;
    bgf:hasValue "1 C"^^cdt:ucum ;
    bgf:parameterValue [
        bgf:varName "C" ;
        bgf:hasValue "20 F"^^cdt:ucum
    ] .

:R
    a bgf:ElectricalResistiveNode ;
    bgf:parameterValue [
        bgf:varName "R" ;
        bgf:hasValue "2 Ohm"^^cdt:ucum
    ] .

:L
    a bgf:ElectricalInductiveNode ;
    bgf:hasValue "0 A"^^cdt:ucum ;
    bgf:parameterValue [
        bgf:varName "L" ;
        bgf:hasValue "10 H"^^cdt:ucum
    ] .

#===============================================================================

:C_R
    bgf:hasSource :C ;
    bgf:hasTarget :R .

:R_L
    bgf:hasSource :R ;
    bgf:hasTarget :L .

#===============================================================================
