@prefix bgf: <https://bg-rdf.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .
@prefix tpl: <https://bg-rdf.org/templates/> .

@prefix : <#> .

#===============================================================================
#
#    C <------ R ------> L
#
# Simulation: 1000 seconds with a 1 second step
#
#===============================================================================

:example_A2
    a bgf:BondgraphModel ;
    bgf:usesTemplate tpl:mechanical.ttl ;
    bgf:hasBondElement :C, :R, :L .

#===============================================================================

:C
    a bgf:MechanicalCapacitiveNode ;
    bgf:hasValue "1 m"^^cdt:ucum ;
    bgf:parameterValue [
        bgf:varName "C" ;
        bgf:hasValue "20 m2.J-1"^^cdt:ucum
    ] .

:R
    a bgf:MechanicalResistiveNode ;
    bgf:parameterValue [
        bgf:varName "R" ;
        bgf:hasValue "0.1 J.s.m-2"^^cdt:ucum
    ] .

:L
    a bgf:MechanicalInductiveNode ;
    bgf:hasValue "0 m/s"^^cdt:ucum ;
    bgf:parameterValue [
        bgf:varName "L" ;
        bgf:hasValue "10 J.s2.m-2"^^cdt:ucum
    ] .

#===============================================================================

:R.C
    bgf:hasSource :R ;
    bgf:hasTarget :C .

:R.L
    bgf:hasSource :R ;
    bgf:hasTarget :L .

#===============================================================================
