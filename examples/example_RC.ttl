@prefix bgf: <https://celldl.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .

@prefix : <#> .

#===============================================================================
#
#      +------- R_0 -------+  V_out
#      |                   |
#     V_in                C_1
#      |                   |
#      +-------------------+
#
#===============================================================================
#
#               R_0       C_1
#                |         |
#                |         |
#     u_0 ------ v ------ u_1
#
#===============================================================================
#
# Simulate for 0.1 sec at 1 ms steps
#
#  V_in = 10 V,  V_out: 0 ---> 10V
#
#===============================================================================

:RC_circuit
    a bgf:BondgraphModel ;
    bgf:hasBondElement :u_0, :R, :q ;
    bgf:hasJunctionStructure :v, :u_1 .

#===============================================================================

:R
    a bgf:ElectricalResistor ;
    bgf:variableValue [
        bgf:varName "R" ;
        bgf:hasValue "10 Ohm"^^cdt:ucum
    ] .

:C
    a bgf:ElectricalCapacitor ;
    bgf:hasValue "0 C"^^cdt:ucum ;
    bgf:variableValue [
        bgf:varName "C" ;
        bgf:hasValue "0.001 F"^^cdt:ucum
    ] .

#===============================================================================

:u_0
    a bgf:VoltageSource ;
    bgf:hasValue "10 V"^^cdt:ucum .

:v
    a bgf:OneNode .

:u_1
    a bgf:ZeroNode .

#===============================================================================

:u_0.v
    bgf:hasSource :u_0 ;
    bgf:hasTarget :v .

:v.R
    bgf:hasSource :v ;
    bgf:hasTarget :R .

:v.u_1
    bgf:hasSource :v ;
    bgf:hasTarget :u_1 .

:u_1.C
    bgf:hasSource :u_1 ;
    bgf:hasTarget :C .

#===============================================================================
