@prefix bgf: <https://bg-rdf.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .

@prefix : <#> .

#===============================================================================
#
#      +------- R_0 -----+----+  V_out
#      |                 |    |
#     V_in              R_1  C_1
#      |                 |    |
#      +-----------------+----+
#
#===============================================================================
#
#               R_0       C_1
#                |         |
#                |         |
#     u_0 ----- v_0 ----- u_1
#                          |
#                          |
#                         R_1
#
#===============================================================================

# Simulate for 0.1 sec at 1 ms steps

#===============================================================================

:R_C_R_circuit
    a bgf:BondgraphModel ;
    bgf:usesTemplate '../schema/elements/electrical.ttl' ;
    bgf:hasBondElement :u_0, :R_0, :R_1, :C_1 ;
    bgf:hasJunctionStructure :v_0, :u_1 .

#===============================================================================

:R_0
    a bgf:ElectricalResistor ;
    bgf:parameterValue [
        bgf:varName "R" ;
        bgf:hasValue "10 Ohm"^^cdt:ucum
    ] .

:R_1
    a bgf:ElectricalResistor ;
    bgf:parameterValue [
        bgf:varName "R" ;
        bgf:hasValue "100 Ohm"^^cdt:ucum
    ] .

:C_1
    a bgf:ElectricalCapacitor ;
    bgf:hasValue "0 C"^^cdt:ucum ;
    bgf:parameterValue [
        bgf:varName "C" ;
        bgf:hasValue "0.001 F"^^cdt:ucum
    ] .

:u_0
    a bgf:VoltageSource ;
    bgf:hasValue "11 V"^^cdt:ucum .

#===============================================================================

:v_0
    a bgf:OneNode .

:u_1
    a bgf:ZeroNode .

#===============================================================================

# PowerBonds implicitly connect to PowerPorts...

:u_0.v_0
    bgf:hasSource :u_0 ;
    bgf:hasTarget :v_0 .

:v_0.R_0
    bgf:hasSource :v_0 ;
    bgf:hasTarget :R_0 .

:v_0.u_1
    bgf:hasSource :v_0 ;
    bgf:hasTarget :u_1 .

:u_1.C_1
    bgf:hasSource :u_1 ;
    bgf:hasTarget :C_1 .

:u_1.R_1
    bgf:hasSource :u_1 ;
    bgf:hasTarget :R_1 .

#===============================================================================
