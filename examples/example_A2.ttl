@prefix bgf: <https://celldl.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .

@prefix : <#> .

#===============================================================================
#
#    q <------ v_1 ------> R
#               |
#               |
#               L
#
#===============================================================================

:example_A2
    a bgf:BondgraphModel ;
    bgf:hasBondElement :q, :R, :L ;
    bgf:hasJunctionStructure :v_1 .

#===============================================================================

:q
    a bgf:MechanicalCapacitor ;
    bgf:hasValue "1 m"^^cdt:ucum ;
    bgf:parameterValue [
        bgf:varName "C" ;
        bgf:hasValue "20 m2.J-1"^^cdt:ucum
    ] .

:R
    a bgf:MechanicalResistor ;
    bgf:parameterValue [
        bgf:varName "R" ;
        bgf:hasValue "0.1 J.s.m-2"^^cdt:ucum
    ] .

:L
    a bgf:MechanicalInductor ;
    bgf:parameterValue [
        bgf:varName "L" ;
        bgf:hasValue "10 J.s2.m-2"^^cdt:ucum
    ] ;
    bgf:variableValue [
        bgf:varName "v" ;
        bgf:hasValue "0 m/s"^^cdt:ucum
    ] .

#===============================================================================

:v_1
    a bgf:OneNode .

#===============================================================================

:v_1.q
    bgf:hasSource :v_1 ;
    bgf:hasTarget :q .

:v_1.R
    bgf:hasSource :v_1 ;
    bgf:hasTarget :R .

:v_1.L
    bgf:hasSource :v_1 ;
    bgf:hasTarget :L .

#===============================================================================
