@prefix bgf: <https://bg-rdf.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .
@prefix tpl: <https://bg-rdf.org/templates/> .

@prefix : <#> .

#===============================================================================
#
#                R_1                    R_2         q_C
#                 |                      |           |
#                 |                      |           |
#    u_in -----> v_1 ------> GY ------> v_2 ------> u_C
#                 |                      |
#                 |                      |
#                L_1                    L_2
#
#===============================================================================

:example_A3
    a bgf:BondgraphModel ;
    bgf:usesTemplate
        tpl:electrical.ttl ,
        tpl:mechanical ;
    bgf:hasBondElement :R_1, :L_1, :R_2, :L_2, :q_C ;
    bgf:hasJunctionStructure :u_in, :v_1, :GY, :v_2, :u_C .

#===============================================================================

:R_1
    a bgf:ElectricalResistor ;
    bgf:parameterValue [
        bgf:varName "R" ;
        bgf:hasValue "10 Ohm"^^cdt:ucum
    ] .

:L_1
    a bgf:ElectricalInductor ;
    bgf:parameterValue [
        bgf:varName "L" ;
        bgf:hasValue "0.2 H"^^cdt:ucum
    ] .

:R_2
    a bgf:MechanicalResistor ;
    bgf:parameterValue [
        bgf:varName "R" ;
        bgf:hasValue "0.4 J.s.m-2"^^cdt:ucum
    ] .

:L_2
    a bgf:MechanicalInductor ;
    bgf:parameterValue [
        bgf:varName "L" ;
        bgf:hasValue "0.01 J.s2.m-2"^^cdt:ucum
    ] .

:q_C
    a bgf:MechanicalCapacitor ;
    bgf:hasValue "0 m"^^cdt:ucum ;
    bgf:parameterValue [
        bgf:varName "E" ;
        bgf:hasValue "100 J.m-2"^^cdt:ucum
    ] .

#===============================================================================

:u_in
    a bgf:ZeroNode .

:v_1
    a bgf:OneNode .

:GY
    a bgf:TransformNode ;
    bgf:hasPort :GY_0, :GY_1 ;
    bgf:parameterValue [
        bgf:varName "K" ;
        bgf:hasValue "6 J.s.C-1.m-1"^^cdt:ucum
    ] .

:v_2
    a bgf:OneNode .

:u_C
    a bgf:ZeroNode .

#===============================================================================

:u_in.v_1
    bgf:hasSource :u_in ;
    bgf:hasTarget :v_1 .

:v_1.R_1
    bgf:hasSource :v_1 ;
    bgf:hasTarget :R_1 .

:v_1.L_1
    bgf:hasSource :v_1 ;
    bgf:hasTarget :L_1 .

:v_1.GY_0
    bgf:hasSource :v_1 ;
    bgf:hasTarget :GY_0 .

:GY_1.v_2
    bgf:hasSource :GY_1 ;
    bgf:hasTarget :v_2 .

:v_2.R_2
    bgf:hasSource :v_2 ;
    bgf:hasTarget :R_2 .

:v_2.L_2
    bgf:hasSource :v_2 ;
    bgf:hasTarget :L_2 .

:v_2.u_C
    bgf:hasSource :v_2 ;
    bgf:hasTarget :u_C .

:u_c.q_C
    bgf:hasSource :u_c ;
    bgf:hasTarget :q_C .

#===============================================================================
