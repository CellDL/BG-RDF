@prefix bg: <https://celldl.org/ontologies/bondgraph#> .
@prefix bgf: <https://celldl.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .

@prefix : <#> .

#===============================================================================
#
#                R_1                    R_2         q_C
#                 |                      |           |
#                 |                      |           |
#    u_in -----> v_1 ------> GY ------> v_2 ------> u_C
#                 |                      |
#                 |                      |
#                L_1                    L_2
#
#===============================================================================

:example_A3
    a bg:BondGraph ;
    bg:hasBondElement :R_1, :L_1, :R_2, :L_2, :q_C ;
    bg:hasJunctionStructure :u_in, :v_1, :GY, :v_2, :u_C ;
    bg:hasPowerBond :u_in.v_1, :v_1.R_1, :v_1.L_1, :v_1.GY_0,
                    :GY_1.v_2, :v_2.R_2, :v_2.L_2, :v_2.u_C, :u_c.q_C .

#===============================================================================

:R_1
    a bgf:Resistance ;
    bgf:hasDomain bgf:Electrical ;
    bgf:variableValue [
        bgf:varName "R" ;
        bgf:hasValue "10 Ohm"^^cdt:ucum
    ] .

:L_1
    a bgf:KineticStore ;
    bgf:hasDomain bgf:Electrical ;
    bgf:variableValue [
        bgf:varName "L" ;
        bgf:hasValue "0.2 H"^^cdt:ucum
    ] .

:R_2
    a bgf:Resistance ;
    bgf:hasDomain bgf:Mechanical ;
    bgf:variableValue [
        bgf:varName "R" ;
        bgf:hasValue "0.4 J.s.m-2"^^cdt:ucum
    ] .

:L_2
    a bgf:KineticStore ;
    bgf:hasDomain bgf:Mechanical ;
    bgf:variableValue [
        bgf:varName "L" ;
        bgf:hasValue "0.01 J.s2.m-2"^^cdt:ucum
    ] .

:q_C
    a bgf:PotentialStore ;
    bgf:hasDomain bgf:Mechanical ;
    bgf:variableValue [
        bgf:varName "E" ;
        bgf:hasValue "100 J.m-2"^^cdt:ucum
    ], [
        bgf:varName "q" ;
        bgf:hasValue "0 m"^^cdt:ucum
    ] .

#===============================================================================

:u_in
    a bgf:ZeroNode .

:v_1
    a bgf:OneNode .

:GY
    a bgf:Transformation ;
    bgf:hasPort :GY_0, :GY_1 ;
    bgf:hasRatio [
        bgf:hasValue "6 J.s.C-1.m-1"^^cdt:ucum
    ] .

:v_2
    a bgf:OneNode .

:u_C
    a bgf:ZeroNode .

#===============================================================================

:u_in.v_1
    bgf:hasSource :u_in ;
    bgf:hasTarget :v_1 .

:v_1.R_1
    bgf:hasSource :v_1 ;
    bgf:hasTarget :R_1 .

:v_1.L_1
    bgf:hasSource :v_1 ;
    bgf:hasTarget :L_1 .

:v_1.GY_0
    bgf:hasSource :v_1 ;
    bgf:hasTarget :GY_0 .

:GY_1.v_2
    bgf:hasSource :GY_1 ;
    bgf:hasTarget :v_2 .

:v_2.R_2
    bgf:hasSource :v_2 ;
    bgf:hasTarget :R_2 .

:v_2.L_2
    bgf:hasSource :v_2 ;
    bgf:hasTarget :L_2 .

:v_2.u_C
    bgf:hasSource :v_2 ;
    bgf:hasTarget :u_C .

:u_c.q_C
    bgf:hasSource :u_c ;
    bgf:hasTarget :q_C .

#===============================================================================
