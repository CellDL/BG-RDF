@prefix bgf: <https://celldl.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .

@prefix : <#> .

#===============================================================================
#
#           C           R           L
#           |           |           |
#           |           |           |
#          u_C ------> v_R ------> u_L
#
#===============================================================================

:example_A1
    a bgf:BondgraphModel ;
    bgf:hasBondElement :C, :R, :L ;
    bgf:hasJunctionStructure :u_C, :v_R, :u_L ;
    bgf:hasPowerBond :u_C.C, :u_C.v_R, :v_R.R, :v_R.u_L, :u_L.L .

#===============================================================================

:C
    a bgf:ElectricalCapacitor ;
    bgf:variableValue [
        bgf:varName "C" ;
        bgf:hasValue "20 F"^^cdt:ucum
    ], [
        bgf:varName "q" ;
        bgf:hasValue "1 C"^^cdt:ucum
    ] .

:R
    a bgf:ElectricalResistor ;
    bgf:variableValue [
        bgf:varName "R" ;
        bgf:hasValue "2 Ohm"^^cdt:ucum
    ] .

:L
    a bgf:ElectricalInductor ;
    bgf:variableValue [
        bgf:varName "L" ;
        bgf:hasValue "10 H"^^cdt:ucum
    ], [
        bgf:varName "v" ;
        bgf:hasValue "0 A"^^cdt:ucum
    ] .

#===============================================================================

:u_C
    a bgf:ZeroNode .

:v_R
    a bgf:OneNode .

:u_L
    a bgf:ZeroNode .

#===============================================================================

:u_C.C
    bgf:hasSource :u_C ;
    bgf:hasTarget :C .

:u_C.v_R
    bgf:hasSource :u_C ;
    bgf:hasTarget :v_R .

:v_R.R
    bgf:hasSource :v_R ;
    bgf:hasTarget :R .

:v_R.u_L
    bgf:hasSource :v_R ;
    bgf:hasTarget :u_L .

:u_L.L
    bgf:hasSource :u_L ;
    bgf:hasTarget :L .

#===============================================================================
