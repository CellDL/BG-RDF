@prefix bg: <https://celldl.org/ontologies/bondgraph#> .
@prefix bgf: <https://celldl.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .

@prefix : <http://example.org#> .

#===============================================================================
#
#      q_1                                    q_2
#       |                                      |
#       |            u_f        u_r            |
#      u_1 ---- v_r ----- Re_r ----- v_r ---- u_2
#
#
#  Is this the same??
#
#      q_1                  q_2
#       |                    |
#       |                    |
#      u_1 ----- Re_r ----- u_2
#
#===============================================================================

:q_1
    a bgf:ChemicalConcentration ;
    bgf:parameterValue [
        a bgf:Value ;
        bgf:hasSymbol "K" ;
        bgf:hasValue "1 mol-1"^^cdt:ucum
    ] ;
    bgf:stateValue [
        a bgf:Value ;
        bgf:hasSymbol "q" ;
        bgf:hasValue "1 mol"^^cdt:ucum
    ] .

:q_2
    a bgf:ChemicalConcentration ;
    bgf:parameterValue [
        a bgf:Value ;
        bgf:hasSymbol "K" ;
        bgf:hasValue "1 mol-1"^^cdt:ucum
    ] ;
    bgf:stateValue [
        a bgf:Value ;
        bgf:hasSymbol "q" ;
        bgf:hasValue "0 mol"^^cdt:ucum
    ] .

:Re_r
    a bgf:ChemicalReaction ;
    bgf:parameterValue [
        a bgf:Value ;
        bgf:hasSymbol "kappa_r" ;
        bgf:hasValue "0.1 mol/s"^^cdt:ucum
    ] .

#===============================================================================

:u_1:
    a bgf:ZeroNode ;
    bgf:hasDomain bgf:Biochemical .

:v_r_1:
    a bgf:OneNode ;
    bgf:hasDomain bgf:Biochemical .

:v_r_2:
    a bgf:OneNode ;
    bgf:hasDomain bgf:Biochemical .

:u_2:
    a bgf:ZeroNode ;
    bgf:hasDomain bgf:Biochemical .

#===============================================================================

:u_1.q_1
    bgf:hasSource :u_1 ;
    bgf:hasTarget :q_1 .

:u_2.q_2
    bgf:hasSource :u_2 ;
    bgf:hasTarget :q_2 .

:u_1.Re_r
    bgf:hasSource :u_1 ;
    bgf:hasTarget [
        bgf:element :Re_r ;
        bgf:port 0
    ] .

:Re_r.u_2
    bgf:hasSource [
        bgf:element :Re_r ;
        bgf:port 1
    ] ;
    bgf:hasTarget :u_1 .

#===============================================================================
