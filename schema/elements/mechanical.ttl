@prefix bgf: <https://celldl.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

#===============================================================================

# Physical System level

#===============================================================================

bgf:Mechanical
    a bgf:ModellingDomain ;
    bgf:hasFlow [
        bgf:varName "v" ;
        bgf:hasUnits "m/s"^^cdt:ucumunit
    ] ;
    bgf:hasPotential [
        bgf:varName "u" ;
        bgf:hasUnits "J/m"^^cdt:ucumunit
    ] ;
    bgf:hasQuantity [
        bgf:varName "q" ;
        bgf:hasUnits "m"^^cdt:ucumunit
    ] .

#===============================================================================

bgf:MechanicalResistor
    a bgf:ElementTemplate ;
    rdfs:subClassOf bgf:Dissipator ;
    bgf:hasDomain bgf:Mechanical ;
    rdfs:label "Mechanical resistor" ;
    bgf:hasVariable [
        bgf:varName "R" ;
        bgf:hasUnits "J.s.m-2"^^cdt:ucumunit
    ] ;
    bgf:constitutiveRelation """
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>u</ci>
                <apply>
                    <times/>
                    <ci>v</ci>
                    <ci>R</ci>
                </apply>
            </apply>
        </math>
    """^^bgf:mathml .

#===============================================================================

bgf:MechanicalCapacitor
    a bgf:ElementTemplate ;
    rdfs:subClassOf bgf:PotentialStore ;
    bgf:hasDomain bgf:Mechanical ;
    rdfs:label "Mechanical capacitor" ;
    bgf:hasVariable [
        bgf:varName "E" ;
        bgf:hasUnits "J.m-2"^^cdt:ucumunit
    ] ;
    bgf:constitutiveRelation """
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q</ci>
                </apply>
                <ci>v</ci>
            </apply>
            <apply>
                <eq/>
                <ci>u</ci>
                <apply>
                    <times/>
                    <ci>E</ci>
                    <ci>q</ci>
                </apply>
            </apply>
        </math>
    """^^bgf:mathml .

#===============================================================================

bgf:MechanicalInductor
    a bgf:ElementTemplate ;
    rdfs:subClassOf bgf:KineticStore ;
    bgf:hasDomain bgf:Mechanical ;
    rdfs:label "Mechanical inductor" ;
    bgf:hasVariable [
        bgf:varName "L" ;
        bgf:hasUnits "J.s2.m-2"^^cdt:ucumunit
    ], [
        bgf:varName "a" ;
        bgf:hasUnits "m/s2"^^cdt:ucumunit
    ] ;
    bgf:constitutiveRelation """
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>v</ci>
                </apply>
                <ci>a</ci>
            </apply>
            <apply>
                <eq/>
                <ci>u</ci>
                <apply>
                    <times/>
                    <ci>L</ci>
                    <ci>a</ci>
                </apply>
            </apply>
        </math>
    """^^bgf:mathml .

#===============================================================================
