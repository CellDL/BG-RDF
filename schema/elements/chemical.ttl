@prefix bgf: <https://celldl.org/ontologies/bondgraph-framework#> .
@prefix cdt: <https://w3id.org/cdt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

#===============================================================================

bgf:Chemical
    a bgf:ModellingDomain ;
    bgf:hasConstant [
        bgf:varName "RT" ;
        bgf:hasValue "2578.73058 J/mol"^^cdt:ucum
    ] ;
    bgf:hasFlow [
        bgf:varName "v" ;
        bgf:hasUnits "mol/s"^^cdt:ucumunit
    ] ;
    bgf:hasPotential [
        bgf:varName "u" ;
        bgf:hasUnits "J/mol"^^cdt:ucumunit
    ] ;
    bgf:hasQuantity [
        bgf:varName "q" ;
        rdfs:label "Chemical quantity (mol)" ;
        bgf:hasUnits "mol"^^cdt:ucumunit
    ] .

#===============================================================================

bgf:ChemicalReaction
    a bgf:ElementTemplate ;
    rdfs:subClassOf bgf:Dissipator ;
    bgf:hasDomain bgf:Chemical ;
    rdfs:label "Chemical reaction" ;
    bgf:hasPort 'f', 'r' ;
    bgf:hasVariable [
        bgf:varName "kappa" ;
        bgf:hasUnits "mol/s"^^cdt:ucumunit
    ] ;
    # v = kappa*(exp(u_f/RT)-exp(u_r/RT))
    bgf:constitutiveRelation """
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>v</ci>
                <apply>
                    <times/>
                    <ci>kappa</ci>
                    <apply>
                        <minus/>
                        <apply>
                            <exp/>
                            <apply>
                                <divide/>
                                <ci>u_f</ci>
                                <ci>RT</ci>
                            </apply>
                        </apply>
                        <apply>
                            <exp/>
                            <apply>
                                <divide/>
                                <ci>u_r</ci>
                                <ci>RT</ci>
                            </apply>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    """^^bgf:mathml .

#===============================================================================

bgf:ChemicalConcentration
    a bgf:ElementTemplate ;
    rdfs:subClassOf bgf:PotentialStore ;
    bgf:hasDomain bgf:Chemical ;
    rdfs:label "Chemical concentration" ;
    bgf:hasVariable [
        bgf:varName "K" ;
        bgf:hasUnits "mol-1"^^cdt:ucumunit
    ] ;
    # ode(q, t) = v
    # u = RT*ln(K*q);
    bgf:constitutiveRelation """
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <ci>v</ci>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>q</ci>
                </apply>
            </apply>
            <apply>
                <eq/>
                <ci>u</ci>
                <apply>
                    <times/>
                    <ci>RT</ci>
                    <apply>
                        <ln/>
                        <apply>
                            <times/>
                            <ci>K</ci>
                            <ci>q</ci>
                        </apply>
                    </apply>
                </apply>
            </apply>
        </math>
    """^^bgf:mathml .

#===============================================================================
#===============================================================================

bgf:ChemicalZeroStorage
    a bgf:CompositeElement ;
    rdfs:label "Chemical zero storage node" ;
    bgf:elementTemplate bgf:ChemicalConcentration ;
    bgf:junctionStructure bgf:ZeroNode .

#===============================================================================

bgf:ChemicalOneReaction
    a bgf:CompositeElement ;
    rdfs:label "Chemical reaction node" ;
    bgf:elementTemplate bgf:ChemicalReaction ;
    bgf:junctionStructure bgf:OneNode .

